# Microsoft Foundry (Azure AI Foundry) Configuration
# Get these values from your Azure AI Foundry project
FOUNDRY_ENDPOINT=https://your-project-name.cognitiveservices.azure.com/
MODEL_DEPLOYMENT_NAME=gpt-4.1

# Azure Subscription
AZURE_SUBSCRIPTION_ID=your-subscription-id

# Azure Retail Prices API (no authentication required)
AZURE_PRICING_API_BASE=https://prices.azure.com/api/retail/prices
AZURE_PRICING_API_VERSION=2023-01-01-preview

# Microsoft Teams (optional - for Teams integration)
MICROSOFT_APP_ID=your-app-id
MICROSOFT_APP_PASSWORD=your-app-password
MICROSOFT_APP_TENANT_ID=your-tenant-id

# Application Settings
DEFAULT_REGION=eastus
LOG_LEVEL=INFO
